import random

def randomList(i):
    list = []
    for j in range(0,i):
        list.append(random.randint(0,100))
    return list


def bubbleSort(list):
    n = len(list)-1
    for i in range(0,len(list)-1):
        for j in range(0,n):
            if list[j] > list[j+1]:
                temp = list[j]
                list[j] = list[j+1]
                list[j+1] = temp
                print(list)
        n = n-1

bubbleSort(randomList(6))
